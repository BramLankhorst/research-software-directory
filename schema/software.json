{
    "$schema": "http://json-schema.org/draft-06/schema",
    "additionalProperties": false,
    "properties": {
        "brandName": {
            "type": "string",
            "maxLength": 50
        },
        "bullets": {
            "type": "string"
        },
        "citationcff": {
            "type": "string"
        },
        "contributors": {
            "items": {
                "allOf": [
                    {
                        "$ref": "person.json"
                    },
                    {
                        "properties": {
                            "affiliations": {
                                "items": {
                                    "type": "string"
                                },
                                "type": "array"
                            },
                            "emailAddress": {
                                "format": "email",
                                "type": "string"
                            },
                            "isContactPerson": {
                                "type": "boolean"
                            }
                        },
                        "type": "object"
                    }
                ]
            },
            "minItems": 1,
            "type": "array"
        },
        "createdAt": {
            "minimum": 0,
            "type": "integer"
        },
        "getStartedURL": {
            "format": "uri",
            "type": "string"
        },
        "githubURLs": {
            "items": {
                "type": "object",
                "properties": {
                    "commits": {
                        "items": {
                            "format": "date-time",
                            "type": "string"
                        },
                        "type": "array"
                    },
                    "isCitationcffSource": {
                        "type": "boolean"
                    },
                    "isCommitDataSource": {
                        "type": "boolean"
                    },
                    "url": {
                        "format": "uri",
                        "type": "string"
                    }
                }
            },
            "type": "array"
        },
        "isCitable": {
            "type": "boolean"
        },
        "isFeatured": {
            "type": "boolean"
        },
        "isPublished": {
            "type": "boolean"
        },
        "license": {
            "type": "array",
            "items": {
                "anyOf": [
                    {
                        "$ref": "license.json"
                    },
                    {
                        "type": "string"
                    }
                ]
            }
        },
        "mentions": {
            "items": {
                "type": "object",
                "properties": {
                    "mentionType": {
                        "enum": [
                            "blogPost",
                            "book",
                            "bookSection",
                            "computerProgram",
                            "conferencePaper",
                            "journalArticle",
                            "newspaperArticle",
                            "presentation",
                            "videoRecording",
                            "webpage"
                        ],
                        "type": "string"
                    },
                    "title": {
                        "type": "string"
                    },
                    "url": {
                        "format": "uri",
                        "type": "string"
                    }
                }
            },
            "type": "array"
        },
        "programmingLanguage": {
            "items": {
                "anyOf": [{
                        "type": "string"
                    },
                    {
                        "type": "string",
                        "enum": [
                            "C",
                            "C#",
                            "C++",
                            "CUDA",
                            "Cython",
                            "Erlang",
                            "Fortran",
                            "FORTRAN",
                            "Go",
                            "Haskell",
                            "HTML",
                            "Java",
                            "JavaScript",
                            "Lisp",
                            "MATLAB",
                            "OpenCL",
                            "Perl",
                            "PHP",
                            "Python",
                            "R",
                            "Ruby",
                            "Shell scripts",
                            "SPARQL",
                            "SQL",
                            "TypeScript",
                            "YAML"
                        ]
                    }
                ]
            },
            "type": "array"
        },
        "shortStatement": {
            "type": "string"
        },
        "readMore": {
            "type": "string"
        },
        "related": {
            "type": "object",
            "properties": {
                "organizations": {
                    "items": {
                        "properties": {
                            "name": {
                                "type": "string"
                            },
                            "logo": {
                                "type": "string"
                            }
                        },
                        "type": "object"
                    },
                    "type": "array"
                },
                "projects": {
                    "items": {
                        "$ref": "project.json"
                    },
                    "type": "array"
                },
                "software": {
                    "items": {
                        "reference": "software"
                    },
                    "type": "array"
                }
            }
        },
        "tags": {
            "items": {
                "$ref": "tag.json"
            },
            "type": "array"
        },
        "testimonial": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "text": {
                        "type": "string"
                    },
                    "attribution": {
                        "allOf": [
                            {
                                "$ref": "person.json"
                            },
                            {
                                "properties": {
                                    "affiliations": {
                                        "items": {
                                            "type": "string"
                                        },
                                        "type": "array"
                                    }
                                },
                                "type": "object"
                            }
                        ]
                    }
                }
            }
        },
        "updatedAt": {
            "type": "integer",
            "minimum": 0
        }
    },
    "type": "object"
}