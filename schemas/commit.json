{
    "$schema": "http://json-schema.org/draft-04/schema",
    "additionalProperties": false,
    "title": "Commit schema",
    "required": [
        "date",
        "repositoryURL"
    ],
    "properties": {
        "date": {
            "type": "string",
            "format": "date-time"
        },
        "repositoryURL": {
            "type": "string",
            "format": "uri"
        }
    }
}