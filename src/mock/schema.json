                "items": {
                "type": "string",
                    "description": "ID of a report, an URI where the reports's metadata can be resolved, e.g., http://software.esciencecenter.nl/report/guide",
                    "format": "uri"
            }
        },
        "logo": {
            "description": "URL to an image of the logo of the organization.",
